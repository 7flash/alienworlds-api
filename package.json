{
  "name": "alienworlds-api",
  "version": "1.0.0",
  "description": "API for AlienWorlds data",
  "main": "index.js",
  "scripts": {
    "test": "npx tap --ts",
    "build": "yarn check-config && yarn clean && tsc && yarn abis",
    "clean": "rm -rf ./dist",
    "abis": "mkdir -p ./dist/abis && node dist/src/fetch_abis.js ./dist/abis",
    "check-config": "node ./scripts/check-config.js || (echo \"Create the config file by copying config.example.js to config.ts\" && exit 1)",
    "mongo-indexes": "node dist/src/mongo_setup.js",
    "api": "node dist/src/server.js",
    "blockrange": "node dist/src/blockrange.js",
    "filler": "node dist/src/filler.js",
    "processor": "node dist/src/processor.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@eosdacio/eosio-statereceiver": "^1.1.0",
    "@eosrio/node-abieos": "^2.1.0-beta.0",
    "@types/node": "^14.14.22",
    "amqplib": "^0.6.0",
    "atomicassets": "^1.2.2",
    "axon": "^2.0.3",
    "commander": "^7.0.0",
    "crypto": "^1.0.1",
    "eosjs": "^21.0.3",
    "ethers": "^5.1.0",
    "fastify": "^3.11.0",
    "fastify-autoload": "^3.4.2",
    "fastify-cors": "^5.2.0",
    "fastify-mongodb": "^2.0.1",
    "fastify-oas": "^3.0.8",
    "file-exists": "^5.0.1",
    "http": "^0.0.1-security",
    "mongodb": "^3.6.3",
    "node-fetch": "^2.6.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "pino-pretty": "^7.2.0",
    "tap": "^15.1.5"
  }
}
